    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.12.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "mockjs": "^1.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-mock": "^3.0.2",
    "vue-tsc": "^2.0.6"
  }
}
